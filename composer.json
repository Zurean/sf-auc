{
    "name": "zurean/sf-auc",
    "description": "Dutch auction based on symfony framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Fertikov",
            "email": "rodeljob@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*",
        "ext-pgsql": "*",
        "doctrine/dbal": "2.10.*",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "3.1.*",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.12",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "roave/security-advisories": "dev-latest",
        "symfony/debug-bundle": "^5.2",
        "symfony/maker-bundle": "^1.30"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
